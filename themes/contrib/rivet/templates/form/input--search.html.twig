{#
/**
 * @file
 * Theme override for a text search element.
 *
 * Available variables:
 * - attributes: A list of HTML attributes for the input element.
 * - children: Optional additional rendered elements.
 *
 * @see template_preprocess_input()
 */
#}
<input{{ attributes.addClass('rvt-text-input').removeClass('form-search') }}/>{{ children }}
